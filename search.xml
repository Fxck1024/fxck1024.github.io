      <url>/posts/f255ffad.html</url>
      
        <content type="html"><![CDATA[<figure class="highlight bash"><table><tr><td class="code"><pre><code class="hljs bash"><span class="hljs-comment">#install</span><br>curl -sSL https://get.docker.com/ | sh<br>systemctl start docker<br>systemctl <span class="hljs-built_in">enable</span> docker<br><span class="hljs-comment">#remove</span><br>docker stop $(docker ps -a -q)<br>docker <span class="hljs-built_in">rm</span> $(docker ps -a -q)<br>docker rmi $(docker images -q)<br></code></pre></td></tr></table></figure>]]></content>
      
      
      
